.js-items-to-slide {
  visibility: hidden;
}

.js-slideing {
  animation-name: slide;
  -webkit-animation-name: slide; 
  animation-duration: 1s; 
  -webkit-animation-duration: 1s;
  visibility: visible;
}

@keyframes slide {
  0% {
    opacity: 0;
    transform: translateY(70%);
  } 
  100% {
    opacity: 1;
    transform: translateY(0%);
  } 
}

@-webkit-keyframes slide {
  0% {
    opacity: 0;
    -webkit-transform: translateY(70%);
  } 
  100% {
    opacity: 1;
    -webkit-transform: translateY(0%);
  }
}

.js-slide_right {
  animation-name: slideright!important;
  -webkit-animation-name: slideright!important; 
  animation-duration: 1s; 
  -webkit-animation-duration: 1s;
  visibility: visible;
}

@keyframes slideright {
  0% {
    opacity: 0;
    transform: translateX(70%);
  } 
  100% {
    opacity: 1;
    transform: translateX(0%);
  } 
}

@-webkit-keyframes slideright {
  0% {
    opacity: 0;
    -webkit-transform: translateX(70%);
  } 
  100% {
    opacity: 1;
    -webkit-transform: translateX(0%);
  }
}

.slide_left {
  animation-name: slideleft;
  -webkit-animation-name: slideleft; 
  animation-duration: 1s; 
  -webkit-animation-duration: 1s;
  visibility: visible;
}

@keyframes slideleft {
  0% {
    opacity: 1;
    transform: translateX(0%);
  } 
  100% {
    opacity: 0;
    transform: translateX(-70%);
  } 
}

@-webkit-keyframes slideleft {
  0% {
    opacity: 1;
    -webkit-transform: translateX(0%);
  } 
  100% {
    opacity: 0;
    -webkit-transform: translateX(-70%);
  }
}

.js-visible {
  visibility: visible!important;
}

.js-invisible {
  visibility: hidden;
}

.inactive-display {
  display: none;
}

.centered {
  padding-left: 0px!important;
  width: 100%!important;
}

input[type=text], select {
  width: 20%;
  padding: 5px;
  display: inline-block;
  border: 1px solid #ccc;
  border-radius: 4px;
  box-sizing: border-box;
  float: right;
}

input[type=text] {
  width: 130px;
  box-sizing: border-box;
  border: 2px solid #ccc;
  border-radius: 4px;
  font-size: 16px;
  background-color: white;
  background-image: url('img/glass.png');
  background-position: 10px 10px; 
  background-repeat: no-repeat;
  background-size: 20px 20px;
  padding: 12px 20px 12px 40px;
  -webkit-transition: width 0.4s ease-in-out;
  transition: width 0.4s ease-in-out;
}

input[type=text]:focus {
  width: 100%;
}

.c-navbar_right_button, .c-navbar_right_button2 {
  background-color: transparent;
  border-style: none;
}

.c-navbar_right_button:hover, .c-navbar_right_button2:hover {
  cursor: pointer;
}

.c-navbar_right_button:active .c-navbar_right_button2:active {
  outline-style: none;
  outline: 0;
  opacity: 0;
}