body {
  overflow-x: hidden;
  margin: 0px;
}

ul {
  position: fixed;
  top: 0;
  width: 100%;
}

li {
 display: inline-block;
}

.c-navbar_right_button--glass {
  width: 40px;
  height: 40px;
  display: block;
}

.c-navbar_right_button--glass-hidden {
  display: none;
}

.c-navbar_right_button2--menubar {
  width: 40px;
  height: 30px;
}

ul.c-navbar {
  list-style-type: none;
  margin: 0;
  padding: 0;
  overflow: hidden;
  background-color: rgba(220, 220, 220, 0.75);
  z-index: 999;
  text-align: center;
}

ul.c-navbar li {
  padding: 50px 20px;
}

ul.c-navbar li.c-navbar_center {
  margin: auto;
  color: #87B343;
  font-size: 43px;
  font-family: 'Open Sans', sans-serif;
  font-weight: 700;
  z-index: 9999;
  padding-left: 382px;
}

ul.c-navbar li.c-navbar_right {
  margin: auto;
  float: right;
}

ul.c-navbar li.c-navbar_right span._add-spacing {
  padding-left: 40px;
  padding-right: 40px;
}

.o-loader-img {
  position: absolute;
  text-align: center;
  margin: auto;
  width: 100%;
  top: 35%;
}

.js-sliding_in {
  animation-name: slidein;
  -webkit-animation-name: slidein; 
  animation-duration: 2s; 
  -webkit-animation-duration: 2s;
  visibility: visible; 
}

@keyframes slidein {
  0% {
    opacity: 0;
    transform: translateY(90%);
  } 
  100% {
    opacity: 1;
    transform: translateY(0%);
  } 
}

@-webkit-keyframes slidein {
  0% {
    opacity: 0;
    -webkit-transform: translateY(90%);
  } 
  100% {
    opacity: 1;
    -webkit-transform: translateY(0%);
  }
} 

.js-sliding_out {
  animation-name: slideout;
  -webkit-animation-name: slideout; 
  animation-duration: 2s; 
  -webkit-animation-duration: 2s;
  visibility: visible; 
}

@keyframes slideout {
  0% {
    opacity: 1;
    transform: translateY(0%);
  } 
  100% {
    opacity: 0;
    transform: translateY(-90%);
  } 
}

@-webkit-keyframes slideout {
  0% {
    opacity: 1;
    -webkit-transform: translateY(0%);
  } 
  100% {
    opacity: 0;
    -webkit-transform: translateY(-90%);
  }
} 

.js-appear {
  display: none;  
  position: relative;
  -webkit-animation-name: animatebottom;
  -webkit-animation-duration: 2s;
  animation-name: animatebottom;
  animation-duration: 1s
}

@-webkit-keyframes animatebottom {
  from { bottom:0px; opacity:0 } 
  to { bottom:0px; opacity:1 }
}

@keyframes animatebottom { 
  from{ bottom:0px; opacity:0 } 
  to{ bottom:0; opacity:1 }
}   

@media screen and (max-width: 1366px){
  .o-phone--sizeing {
    width: 600px!important;
    height: 700px!important;
    top: -550px!important;
    right: -100px!important;
  }
  .c-navbar_right_button--glass {
    width: 30px;
    height: 30px;
  }
 .c-navbar_right_button2--menubar {
    width: 30px;
    height: 20px;
  }
  ul.c-navbar li {
    padding: 25px 20px;
  }
  ul.c-navbar li.c-navbar_center {
    font-size: 33px!important;
  }
}

@media screen and (max-width: 1050px){
  .o-phone--sizeing {
    width: 500px!important;
    height: 600px!important;
    top: -450px!important;
    right: -100px!important;
  }
  .c-navbar_right {
    float: none!important;
    display: none!important;
  }
  .c-navbar_center {
    padding-left: 20px!important;
  }
  ul {
    width: 100%;
    height: 94px;
  }
  h1 {
    font-size: 25px!important;
