}

@media screen and (max-width: 751px){
  h1 {
    font-size: 25px!important;
    width: 100%!important;
  }
  p {
    width: 100%!important;
    font-size: 14px!important;
  }
  td {
    width: 100%!important;
  }
  ul {
    width: 100%;
    height: 94px;
  }
  ul.c-navbar li.c-navbar_center {
    padding-left: 20px;
  }
  ._inactive-display-mobile {
    display: none!important;
  }
  .c-navbar_right {
    visibility: hidden;
  }
  .c-mainpark_jumbo_table--paragraph {
    width: 60%!important;
  }
  .c-mainpark {
    background: url("img/parksmall.png") no-repeat top center!important;
    padding: 250px 0px!important;
  }
  .c-mainpark_jumbo {
    padding-top: 0px!important;
  }
  .o-phone--sizeing {
    width: 300px!important;
    height: 400px!important;
    top: -300px!important;
    right: -50px!important;
  }
  .c-thirdpark_table--color1 {
    background-image: url("img/graysmall.png");
  }
  .c-thirdpark_table--color2 {
    background-image: url("img/greensmall.png");
  }
  .c-thirdpark_table--color3 {
    background-image: url("img/navysmall.png");
  }
  .c-thirdpark_table--color4 {
    background-image: url("img/lightgreensmall.png");
  }
  table, td {
    display: block;
  }
  td {
    position: relative;
    padding-left: 45%;
  }
  td:before {
    position: absolute;
    width: 45%;
